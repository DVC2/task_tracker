{
  "tasktracker.commands": {
    "listTasks": "tasktracker list",
    "viewTask": "tasktracker view ${input:taskId}",
    "addTask": "tasktracker quick \"${input:taskTitle}\" \"${input:taskCategory}\"",
    "updateTaskStatus": "tasktracker update ${input:taskId} status ${input:taskStatus}"
  },
  "tasktracker.statusBar": {
    "enabled": true,
    "command": "tasktracker list --current",
    "refreshInterval": 60
  }
} 